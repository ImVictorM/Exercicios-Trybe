use('cinema');

// ex 1
db.movies.find({
  category: { $all: ['action', 'adventure'] }
});

// ex 2
db.movies.find({
  category: { $all: ['action', 'sci-fi'] },
  imdbRating: { $gt: 7 }
});

// ex 3
db.movies.find({
  ratings: { $gt: 103 }
}, {
  _id: 0,
  title: 1,
  ratings: 1
});

// ex 4
db.movies.find({
  ratings: { $elemMatch: { $gte: 100, $lte: 105 } }
}, {
  _id: 0,
  title: 1,
  ratings: 1
});

